<?xml version="1.0"?>

<robot name="cuboid" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="cuboid_gazebo" params="name">

    <gazebo reference="${name}_link">
      <selfCollide>true</selfCollide>
      <material>Gazebo/Red</material>
      <mu1>1</mu1>
      <mu2>1</mu2>
      <kp value="1e6" />
      <kd value="1e3" />      
    </gazebo>

  </xacro:macro>

</robot>

